%h1 Add Couch
%br
= form_for [@user, @couch] do |f|
  = f.label :name
  %br
  = f.text_field :name
  %br
  = f.label :description
  %br
  = f.text_area :description
  %br
  = f.label :street_address
  %br
  = f.text_area :street_address
  %br
  = f.label :city
  %br
  = f.text_area :city
  %br
  = f.label :state
  %br
  = f.text_area :state
  %br
  = f.label :zipcode
  %br
  = f.text_area :zipcode
  %br
  = f.submit "Add Couch"